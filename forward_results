#!/bin/bash

arg0="${0##*/}"

LOCK=/dev/shm/.lck.$arg0

exec 9<>$LOCK

flock -n 9 || exit 2

TS_FILE="$1"
shift

(
  rsync -e 'ssh -i /var/www/creds/web_push -p 2122'  "$@"  hsv_timing@www.dd.id.au:Timing_Results/ &&
   touch -r "$1" "${TS_FILE}"
  sleep 100 # Dont run another transfer for 2 mins or so
)&

